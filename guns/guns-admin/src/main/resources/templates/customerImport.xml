<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="customerData" var="customer"
              varType="com.stylefeng.guns.modular.system.model.Customer">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">customer.name</mapping>
                <mapping row="1" col="1">customer.phone</mapping>
                <mapping row="1" col="2">customer.balance</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>